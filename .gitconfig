[user]
	email = calderongilberto3@gmail.com
	name = Gilberto
[init]
	defaultBranch = master
[credential]
	helper = store
[color]
  diff = auto
  status = auto
  branch = auto
[color "status"]
  added = green
  changed = blue
  untracked = red 
[color "diff"]
  meta = yellow
  old = red 
  new = green
[alias]
  st = status
  ci = commit
  co = checkout
  tm = merge --no-ff --no-commit
  pp = !git pull && git push
  rm-untracked = !rm $(git ls-files --other --exclude-standard)
  lsblame = !cd "./$GIT_PREFIX" && ls -A | xargs -n1 -I'{}' git log --no-merges --format='%h (%an%x09%ai) {}' -1 '{}' | column -ts $'\t'
  df = diff
[core]
  editor = /usr/bin/vim

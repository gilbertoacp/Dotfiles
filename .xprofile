export _JAVA_AWT_WM_NONREPARENTING=1

# Stop screen from going blank
xset s off
xset -dpms
xset s noblank

# Faster key repeat and delay
xset r rate 210 40

# Compositor
picom &

# Keyboard Layout
setxkbmap us &

# Volume icon
volumeicon &

# Mouse wheel 
imwheel &

# Background
nitrogen --restore &
